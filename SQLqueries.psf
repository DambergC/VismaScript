	$RichtextboxSQLQueries.AppendText("`n")
	$RichtextboxSQLQueries.AppendText("$NeptuneUser ")
	$RichtextboxSQLQueries.AppendText("`n")
	$RichtextboxSQLQueries.AppendText("$QuickReportAdmin	")
